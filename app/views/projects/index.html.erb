<div class="row">
    <div class="col-lg-12 text-center">
        <h1>Welcome back!</h1>
    </div>
</div>
<div class="row">
    <div class="col-lg-12 text-center">
        <h3>Here are your current projects:</h3>
    </div>
</div>
<div class="row">
    <% @projects.each do |project| %>
        <div class="col-lg-4">
            <h4><%= project.title %></h4>
            <p><%= project.created_at_formatted %></p>
            <p><%= project.description %></p>
            <%= link_to "Edit", edit_project_path(project) %>
        </div>
    <% end %>
</div>